{
  "name": "cobib",
  "version": "0.0.0",
  "description": "A game-based collaborative bibliography zotero plugin",
  "main": "bundle.js",
  "dependencies": {
    "d3": "~3.3.11",
    "underscore": "~1.5.2",
    "browserify-shim": "~3.0.4",
    "browserify": "~3.0.0",
    "deamdify": "~0.1.1",
    "debowerify": "~0.4.0",
    "watchify": "~0.4.1",
    "oauth": "~0.9.11",
    "URIjs": "~1.12.0"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "deamdify"
    ]
  },
  "browser": {
    "jQuery": "./bower_components/jquery/jquery.min.js",
    "bootstrap": "./bower_components/bootstrap/dist/js/bootstrap.js",
    "angular": "./bower_components/angular/angular.js"
  },
  "browserify-shim": {
    "angular": "angular",
    "bootstrap": {
      "exports": "bootstrap",
      "depends": [
        "jQuery:jQuery"
      ]
    }
  },
  "scripts": {
    "build": "browserify -x jQuery -x bootstrap -x angular -x d3 js/app.js > bundle.js",
    "libs": "browserify -r jQuery -r bootstrap -r angular -r d3 > libs.js",
    "watch": "watchify -x jQuery -x bootstrap -x angular -x d3 js/app.js -o bundle.js -dv",
    "start": "python -m SimpleHTTPServer"
  },
  "repository": "",
  "author": ""
}
